package ex05;

public class 중괄호블럭 {

	public static void main(String[] args) {
		int n = 100;
		n = 200;
		System.out.println(n);
		{
			int num = 10;
			System.out.println(num);
		}

		{
			int num2 = 20;
			System.out.println(num2);
		}
		
		{
			int num2 = 30;
			System.out.println(num2);
		}
	}

}
